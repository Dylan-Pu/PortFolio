{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php@1.1.0"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "public/build" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production"
  }
}